class Solution {
    public int search(int[] nums, int target) {
        int i=0;
        int j=nums.length-1;
        int in=-1;
        while(i<=j){
            if(nums[i]==target){
                in=i;
            }else if(nums[j]==target){
                in=j;
            }
            i++;
            j--;
        }return in;
    
    }
}

class Solution:
    def romanToInt(self, s: str) -> int:
        a = {"I":1, "V":5, "X":10, "L":50, "C":100, "D":500, "M":1000}

        c = 0
        for i in range(len(s)):
            if s[i] in a.keys():
                if i+1<len(s) and a[s[i]]<a[s[i+1]]:
                    c=c-a[s[i]]
                else:
                    c=c+a[s[i]]
        return c
